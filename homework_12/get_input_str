def get_input_str(prompt=None, options=None):
    if prompt is None:
        prompt = 'Enter something: '
    a = ''
    while not a:
        a = input(prompt)
        if options:
            if a in options:
                print('this key is exist')
                break
            else:
                print('Error')




some_dict = {'a': 'User guesses', 'b': 'Program guesses'}


get_input_str()

