def get_input_str(prompt=None, options=None):
    my_libr = [{1: 'yes', 2: 'no'}, {3: 'true', 4: 'false'}]
    while True:
        a = input(prompt)
        if a in my_libr[options].values():
            return True
        else:
            print('Try again')


def one():
    if get_input_str(prompt='Enter : ', options=0):
        print('all ok')


one()
print('it is working')
